if len(str) > 10:  
    print(str[:5])  
else:   
    print(str)

#–í–∞—Ä–∏–∞–Ω—Ç 12
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ2
pc_num = 10  
user_num = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: "))  
if user_num == pc_num or user_num == pc_num + 1 or user_num == pc_num - 1:  
    print(True)  
else:  
    print(False)  
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ3
g = (221 - 13) * 2  
a = int(input("–í–≤–µ–¥–∏—Ç–µ –≤–∞—à –æ—Ç–≤–µ—Ç: "))  
if a == g:  
    print(True)  
elif a > g:  
    print(">")  
else:  
    print("<") 
