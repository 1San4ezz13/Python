#1
text = 'important information in one line'
letter = input("–í–≤–µ–¥–∏—Ç–µ –±—É–∫–≤—É: ").strip().lower()  
if len(letter) == 1:  
    result = letter in text  
    print(result)  
else:  
    print("–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —Ä–æ–≤–Ω–æ –æ–¥–Ω—É –±—É–∫–≤—É") 
#2
def calculate_figure():
    print("=== –û–ø—Ä–µ–¥–µ–ª–∏—Ç–µ–ª—å —Ñ–∏–≥—É—Ä—ã –∏ –ø–ª–æ—â–∞–¥–∏ ===")
    
    try:
        side1 = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –ø–µ—Ä–≤–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        side2 = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –≤—Ç–æ—Ä–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        if side1 <= 0 or side2 <= 0:
            print("‚ùå –û—à–∏–±–∫–∞: —Å—Ç–æ—Ä–æ–Ω—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã–º–∏ —á–∏—Å–ª–∞–º–∏!")
            return
        if side1 == side2:
            figure_type = "–ö–í–ê–î–†–ê–¢"
        else:
            figure_type = "–ü–†–Ø–ú–û–£–ì–û–õ–¨–ù–ò–ö"
        area = side1 * side2
        print(f"\nüìê –§–∏–≥—É—Ä–∞: {figure_type}")
        print(f"üìè –°—Ç–æ—Ä–æ–Ω–∞ A: {side1}")
        print(f"üìè –°—Ç–æ—Ä–æ–Ω–∞ B: {side2}")
        print(f"üìä –ü–ª–æ—â–∞–¥—å: {area}")
        
    except ValueError:
        print("‚ùå –û—à–∏–±–∫–∞: –ø–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ–≤—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è!")
def detailed_figure_calculation():
    print("=== –û–ø—Ä–µ–¥–µ–ª–∏—Ç–µ–ª—å —Ñ–∏–≥—É—Ä—ã –∏ –ø–ª–æ—â–∞–¥–∏ ===")
    
    try:
        a = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –ø–µ—Ä–≤–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        b = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –≤—Ç–æ—Ä–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        
        if a <= 0 or b <= 0:
            print("‚ùå –û—à–∏–±–∫–∞: —Å—Ç–æ—Ä–æ–Ω—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã–º–∏ —á–∏—Å–ª–∞–º–∏!")
            return
        if a == b:
            figure_type = "–ö–í–ê–î–†–ê–¢"
            description = f"–í—Å–µ —Å—Ç–æ—Ä–æ–Ω—ã —Ä–∞–≤–Ω—ã ({a})"
        else:
            figure_type = "–ü–†–Ø–ú–û–£–ì–û–õ–¨–ù–ò–ö"
            description = f"–°–º–µ–∂–Ω—ã–µ —Å—Ç–æ—Ä–æ–Ω—ã: {a} –∏ {b}"
        area = a * b
        perimeter = 2 * (a + b)
        print(f"\n{'='*40}")
        print(f"üìê –§–ò–ì–£–†–ê: {figure_type}")
        print(f"{'='*40}")
        print(f"üìè –°—Ç–æ—Ä–æ–Ω–∞ A: {a}")
        print(f"üìè –°—Ç–æ—Ä–æ–Ω–∞ B: {b}")
        print(f"üìê –•–∞—Ä–∞–∫—Ç–µ—Ä–∏—Å—Ç–∏–∫–∞: {description}")
        print(f"üìä –ü–ª–æ—â–∞–¥—å: {area}")
        print(f"üìè –ü–µ—Ä–∏–º–µ—Ç—Ä: {perimeter}")
        if figure_type == "–ö–í–ê–î–†–ê–¢":
            print(f"üí° –î–∏–∞–≥–æ–Ω–∞–ª—å: {a * (2**0.5):.2f}")
    except ValueError:
        print("‚ùå –û—à–∏–±–∫–∞: –ø–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ–≤—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è!")
def interactive_figure_detector():
    print("=== –û–ø—Ä–µ–¥–µ–ª–∏—Ç–µ–ª—å —Ñ–∏–≥—É—Ä –∏ –ø–ª–æ—â–∞–¥–µ–π ===")
    print("–î–ª—è –≤—ã—Ö–æ–¥–∞ –≤–≤–µ–¥–∏—Ç–µ 'exit'\n")
    while True:
        try:
            user_input = input("–í–≤–µ–¥–∏—Ç–µ –¥–≤–µ —Å—Ç–æ—Ä–æ–Ω—ã —á–µ—Ä–µ–∑ –ø—Ä–æ–±–µ–ª: ").strip()
            if user_input.lower() == 'exit':
                print("–î–æ —Å–≤–∏–¥–∞–Ω–∏—è!")
                break
            if not user_input:
                continue
            sides = user_input.split()
            if len(sides) != 2:
                print("‚ùå –û—à–∏–±–∫–∞: –Ω—É–∂–Ω–æ –≤–≤–µ—Å—Ç–∏ —Ä–æ–≤–Ω–æ –¥–≤–∞ —á–∏—Å–ª–∞!")
                continue
            a = float(sides[0])
            b = float(sides[1])
            if a <= 0 or b <= 0:
                print("‚ùå –û—à–∏–±–∫–∞: —Å—Ç–æ—Ä–æ–Ω—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã–º–∏!")
                continue
            if a == b:
                figure = "–ö–í–ê–î–†–ê–¢"
                emoji = "‚¨ú"
            else:
                figure = "–ü–†–Ø–ú–û–£–ì–û–õ–¨–ù–ò–ö"
                emoji = "üü¶"
            area = a * b
            perimeter = 2 * (a + b)
            print(f"{emoji} –§–∏–≥—É—Ä–∞: {figure}")
            print(f"   –°—Ç–æ—Ä–æ–Ω—ã: {a} √ó {b}")
            print(f"   –ü–ª–æ—â–∞–¥—å: {area}")
            print(f"   –ü–µ—Ä–∏–º–µ—Ç—Ä: {perimeter}")
            print()
        except ValueError:
            print("‚ùå –û—à–∏–±–∫–∞: –ø–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ–≤—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è!")
        except KeyboardInterrupt:
            print("\n\n–ü—Ä–æ–≥—Ä–∞–º–º–∞ –∑–∞–≤–µ—Ä—à–µ–Ω–∞.")
            break
def simple_figure_check():
    try:
        a = float(input("–í–≤–µ–¥–∏—Ç–µ –ø–µ—Ä–≤—É—é —Å—Ç–æ—Ä–æ–Ω—É: "))
        b = float(input("–í–≤–µ–¥–∏—Ç–µ –≤—Ç–æ—Ä—É—é —Å—Ç–æ—Ä–æ–Ω—É: "))        
        if a <= 0 or b <= 0:
            print("–°—Ç–æ—Ä–æ–Ω—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã–º–∏!")
            return
        if a == b:
            print(f"–ö–≤–∞–¥—Ä–∞—Ç —Å–æ —Å—Ç–æ—Ä–æ–Ω–æ–π {a}, –ø–ª–æ—â–∞–¥—å = {a*b}")
        else:
            print(f"–ü—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫ {a}√ó{b}, –ø–ª–æ—â–∞–¥—å = {a*b}")
    except ValueError:
        print("–û—à–∏–±–∫–∞ –≤–≤–æ–¥–∞!")
if __name__ == "__main__":
    calculate_figure()
    detailed_figure_calculation()
    interactive_figure_detector()
    simple_figure_check()
#3
def check_mood(response):   
    positive = ['—Ö–æ—Ä–æ—à–æ', '–Ω–æ—Ä–º–∞–ª—å–Ω–æ', '–æ—Ç–ª–∏—á–Ω–æ']   
    negative = ['–ø–ª–æ—Ö–æ', '–Ω–µ —Ö–æ—Ä–æ—à–æ']   
    response = response.lower()  
    if response in positive:  
        return 'üòä'  
    elif response in negative:  
        return 'üôÅ'  
    else:  
        return 'üòê' 
#–í–∞—Ä–∏–∞–Ω—Ç 5
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ1
num1 = float(input("–í–≤–µ–¥–∏—Ç–µ –ø–µ—Ä–≤–æ–µ —á–∏—Å–ª–æ: "))
num2 = float(input("–í–≤–µ–¥–∏—Ç–µ –≤—Ç–æ—Ä–æ–µ —á–∏—Å–ª–æ: "))
if num1 > num2:
    result = num1 ** num2
    print(f"–ü–µ—Ä–≤–æ–µ —á–∏—Å–ª–æ ({num1}) –±–æ–ª—å—à–µ. –†–µ–∑—É–ª—å—Ç–∞—Ç –≤–æ–∑–≤–µ–¥–µ–Ω–∏—è –≤ —Å—Ç–µ–ø–µ–Ω—å: {result}")
elif num2 > num1:
     result = num2 ** num1
     print(f"–í—Ç–æ—Ä–æ–µ —á–∏—Å–ª–æ ({num2}) –±–æ–ª—å—à–µ. –†–µ–∑—É–ª—å—Ç–∞—Ç –≤–æ–∑–≤–µ–¥–µ–Ω–∏—è –≤ —Å—Ç–µ–ø–µ–Ω—å: {result}")
else:
    result = num1 + num2
    print(f"–ß–∏—Å–ª–∞ —Ä–∞–≤–Ω—ã ({num1} –∏ {num2}). –ò—Ö —Å—É–º–º–∞: {result}")
    print("–û—à–∏–±–∫–∞ –≤–≤–æ–¥–∞. –ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ –∫–æ—Ä—Ä–µ–∫—Ç–Ω—ã–µ —á–∏—Å–ª–∞.")
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ2
new_message = "Hello! How are you?"
response = input("–í–≤–µ–¥–∏—Ç–µ –≤–∞—à –æ—Ç–≤–µ—Ç –Ω–∞ —Å–æ–æ–±—â–µ–Ω–∏–µ 'Hello! How are you?': ")
if new_message and response:
    if new_message[0] == response[0]:
        print(True)
    else:
        print(False)
else:
    print("–û–¥–Ω–∞ –∏–∑ —Å—Ç—Ä–æ–∫ –ø—É—Å—Ç–∞, —Å—Ä–∞–≤–Ω–µ–Ω–∏–µ –Ω–µ–≤–æ–∑–º–æ–∂–Ω–æ.")
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ3
length1 = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –ø–µ—Ä–≤–æ–≥–æ –æ—Ç—Ä–µ–∑–∫–∞: "))
length2 = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –≤—Ç–æ—Ä–æ–≥–æ –æ—Ç—Ä–µ–∑–∫–∞: "))
if length1 > length2:
    difference = length1 - length2
    print(f"–ü–µ—Ä–≤—ã–π –æ—Ç—Ä–µ–∑–æ–∫ –¥–ª–∏–Ω–Ω–µ–µ –≤—Ç–æ—Ä–æ–≥–æ –Ω–∞ {difference} –µ–¥–∏–Ω–∏—Ü.")
elif length2 > length1:
    difference = length2 - length1
    print(f"–í—Ç–æ—Ä–æ–π –æ—Ç—Ä–µ–∑–æ–∫ –¥–ª–∏–Ω–Ω–µ–µ –ø–µ—Ä–≤–æ–≥–æ –Ω–∞ {difference} –µ–¥–∏–Ω–∏—Ü.")
elif length1 == length2:
    print("–û—Ç—Ä–µ–∑–∫–∏ —Ä–∞–≤–Ω—ã –ø–æ –¥–ª–∏–Ω–µ.")
else:
    print("–û—à–∏–±–∫–∞ –≤–≤–æ–¥–∞. –ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ –∫–æ—Ä—Ä–µ–∫—Ç–Ω—ã–µ —á–∏—Å–ª–∞.")
#–í–∞—Ä–∏–∞–Ω—Ç 6
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ1
str = input("–í–≤–µ–¥–∏—Ç–µ –ø—Ä–æ–∏–∑–≤–æ–ª—å–Ω—É—é —Å—Ç—Ä–æ–∫—É: ")
if len(str) > 0:
    first_char = str[0]
    last_char = str[-1]
    if first_char == last_char:
        print(True)
    else:
        print(False)
else:
    print(False)
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ2
number = int(input("–í–≤–µ–¥–∏—Ç–µ —Ü–µ–ª–æ–µ —á–∏—Å–ª–æ: "))
if number % 2 == 0:
    result = number ** 2
    print(f"–ß–∏—Å–ª–æ –∫—Ä–∞—Ç–Ω–æ 2. –†–µ–∑—É–ª—å—Ç–∞—Ç (—á–∏—Å–ª–æ –≤ —Å—Ç–µ–ø–µ–Ω–∏ 2): {result}")
elif number % 3 == 0:
    result = number ** 3
    print(f"–ß–∏—Å–ª–æ –∫—Ä–∞—Ç–Ω–æ 3. –†–µ–∑—É–ª—å—Ç–∞—Ç (—á–∏—Å–ª–æ –≤ —Å—Ç–µ–ø–µ–Ω–∏ 3): {result}")
else:
    result = number * 100
    print(f"–ß–∏—Å–ª–æ –Ω–µ –∫—Ä–∞—Ç–Ω–æ –Ω–∏ 2, –Ω–∏ 3. –†–µ–∑—É–ª—å—Ç–∞—Ç (—á–∏—Å–ª–æ * 100): {result}")
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ3
num1 = float(input("–í–≤–µ–¥–∏—Ç–µ –ø–µ—Ä–≤–æ–µ —á–∏—Å–ª–æ: "))
num2 = float(input("–í–≤–µ–¥–∏—Ç–µ –≤—Ç–æ—Ä–æ–µ —á–∏—Å–ª–æ: "))
if num1 < 0 and num2 < 0:
    print(False)
elif num1 >= 0 and num2 >= 0:
    print(True)
elif num1 < 0:
    num1 += 1000  
    print(f"–ü–µ—Ä–≤–æ–µ —á–∏—Å–ª–æ (–æ—Ç—Ä–∏—Ü–∞—Ç–µ–ª—å–Ω–æ–µ) —Å–∫–æ—Ä—Ä–µ–∫—Ç–∏—Ä–æ–≤–∞–Ω–æ: {num1}")
    print(f"–í—Ç–æ—Ä–æ–µ —á–∏—Å–ª–æ (–ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω–æ–µ): {num2}")
elif num2 < 0:
        num2 += 1000  
        print(f"–ü–µ—Ä–≤–æ–µ —á–∏—Å–ª–æ (–ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω–æ–µ): {num1}")
        print(f"–í—Ç–æ—Ä–æ–µ —á–∏—Å–ª–æ (–æ—Ç—Ä–∏—Ü–∞—Ç–µ–ª—å–Ω–æ–µ) —Å–∫–æ—Ä—Ä–µ–∫—Ç–∏—Ä–æ–≤–∞–Ω–æ: {num2}")
#–í–∞—Ä–∏–∞–Ω—Ç 7
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ1
str = input("–í–≤–µ–¥–∏—Ç–µ –ø—Ä–æ–∏–∑–≤–æ–ª—å–Ω—É—é —Å—Ç—Ä–æ–∫—É: ")
target_chars = ['—è', '–∏', '–µ', '—é']
if len(str) > 0:
    last_char = str[-1]
    if last_char in target_chars:
        print(True)
    else:
        print(False)
else:
    print(False)
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ2
a = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –ø–µ—Ä–≤–æ–π —Å—Ç–æ—Ä–æ–Ω—ã (a): "))
b = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –≤—Ç–æ—Ä–æ–π —Å—Ç–æ—Ä–æ–Ω—ã (b): "))
c = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É —Ç—Ä–µ—Ç—å–µ–π —Å—Ç–æ—Ä–æ–Ω—ã (c): "))
positive_sides = (a > 0) and (b > 0) and (c > 0)
triangle_inequality = (a + b > c) and (a + c > b) and (b + c > a)
if positive_sides and triangle_inequality:
    print(True)
else:
    print(False)
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ4
number = int(input("–í–≤–µ–¥–∏—Ç–µ —Ü–µ–ª–æ–µ —á–∏—Å–ª–æ: "))
last_digit = number % 10
if last_digit == 0:
    result = number ** 10
    print(f"–ü–æ—Å–ª–µ–¥–Ω—è—è —Ü–∏—Ñ—Ä–∞ 0. –†–µ–∑—É–ª—å—Ç–∞—Ç (—á–∏—Å–ª–æ^10): {result}")
elif last_digit == 1:
    result = number % 3
    print(f"–ü–æ—Å–ª–µ–¥–Ω—è—è —Ü–∏—Ñ—Ä–∞ 1. –†–µ–∑—É–ª—å—Ç–∞—Ç (—á–∏—Å–ª–æ % 3): {result}")
elif last_digit == 2:
    result = number // 2
    print(f"–ü–æ—Å–ª–µ–¥–Ω—è—è —Ü–∏—Ñ—Ä–∞ 2. –†–µ–∑—É–ª—å—Ç–∞—Ç (—á–∏—Å–ª–æ // 2): {result}")
elif last_digit >= 3:
    result = number ** 2
    print(f"–ü–æ—Å–ª–µ–¥–Ω—è—è —Ü–∏—Ñ—Ä–∞ >= 3. –†–µ–∑—É–ª—å—Ç–∞—Ç (—á–∏—Å–ª–æ^2): {result}")
#–í–∞—Ä–∏–∞–Ω—Ç 8
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ1
password = input("–í–≤–µ–¥–∏—Ç–µ –ø–∞—Ä–æ–ª—å: ")
if len(password) < 8 or password == "qwerty123":
    print(False)
else:
    print(True)
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ2
pc_number = 777
user_num1 = float(input("–í–≤–µ–¥–∏—Ç–µ –ø–µ—Ä–≤–æ–µ —á–∏—Å–ª–æ: "))
user_num2 = float(input("–í–≤–µ–¥–∏—Ç–µ –≤—Ç–æ—Ä–æ–µ —á–∏—Å–ª–æ: "))
condition1 = (user_num1 < pc_number and user_num2 > pc_number)
condition2 = (user_num2 < pc_number and user_num1 > pc_number)
if condition1 or condition2:
    print(True)
else:
    print(False)
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ3
lamp_1 = 0
lamp_2 = 0
user_choice = input("–ö–∞–∫—É—é –ª–∞–º–ø–æ—á–∫—É –∑–∞–∂–µ—á—å? (–í–≤–µ–¥–∏—Ç–µ '1' –∏–ª–∏ '2'): ")
if user_choice == "1":
    lamp_1 = 1
    print(f"–õ–∞–º–ø–æ—á–∫–∞ 1 –∑–∞–∂–∂–µ–Ω–∞. –°—Ç–∞—Ç—É—Å: lamp_1 = {lamp_1}, lamp_2 = {lamp_2}")
elif user_choice == "2":
    lamp_2 = 1
    print(f"–õ–∞–º–ø–æ—á–∫–∞ 2 –∑–∞–∂–∂–µ–Ω–∞. –°—Ç–∞—Ç—É—Å: lamp_1 = {lamp_1}, lamp_2 = {lamp_2}")
else:
    print("–û–±–µ –ª–∞–º–ø–æ—á–∫–∏ –Ω–µ –≥–æ—Ä—è—Ç.")
    print(f"–°—Ç–∞—Ç—É—Å: lamp_1 = {lamp_1}, lamp_2 = {lamp_2}")

#–í–∞—Ä–∏–∞–Ω—Ç ‚Ññ9 
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ1
switch_1 = False
switch_2 = False
print("–í–∫–ª—é—á–∏—Ç—å?")
text = input("–í–≤–µ–¥–∏—Ç–µ –¥–∞ –∏–ª–∏ –Ω–µ—Ç")
if text == "–¥–∞":
    switch_1 = True
    switch_2 = True
    print("–í—Å—ë –≤–∫–ª—é—á–µ–Ω–æ")
else:
    print(switch_1, switch_2)

#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ2
number = int(input("–í–≤–µ–¥–∏—Ç–µ —Ü–µ–ª–æ–µ —á–∏—Å–ª–æ: "))
if number % 2 == 0 and number >=0:
    print("True, ¬´even¬ª")
elif number % 2 == 1 and number >=0:
    print("True,¬´odd¬ª")
else:
    print(False)

#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ3
str = input("–í–≤–µ–¥–∏—Ç–µ —Å—Ç—Ä–æ–∫—É")
if str.startswith('/'):  
    print("¬´command¬ª")
else:
    print("¬´It‚Äôs string¬ª")

#–í–∞—Ä–∏–∞–Ω—Ç ‚Ññ10
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ1
def check_string_length(s):  
    if len(s) == 0:  
        print(None)  
    elif len(s) <= 5:  
        print("short")   
    elif 6 <= len(s) <= 10:  
        print("normal") 
    else:  
        print("long") 

#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ2
number = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ"))
if number < 0: 
    number = 1_000_000
    print(number)
elif number == 0:
    number = 2 ** 2
    print(number)
else:
    number = 2 ** 3
    print(number)

#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ3
number_1 = 10
number_2 = 100
number_3 = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ"))
if number_3 > number_1 and number_3 < number_2:
    print(True)
else:
    print(False)

#–í–∞—Ä–∏–∞–Ω—Ç ‚Ññ11
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ1
prog_num = 0
number_1 = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ"))
number_2 = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ"))
if number_1 < 0 and number_2 < 0:
    prog_num = number_1 + number_2
    print(prog_num)
elif number_1 > 0 and number_2 > 0:
    prog_num = number_1 - number_2
    print(prog_num)
elif number_1 < 0 and number_2 > 0:
    print(False)
elif number_1 > 0 and number_2 < 0:
    print(False)
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ2 
number_1 = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ"))
if number_1 % 2 == 1:
    number_1 += 1
    print(number_1)
else:
    print(True)

#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ3
