#–í–∞—Ä–∏–∞–Ω—Ç 1
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ1
num = int(input("–í–≤–µ–¥–∏—Ç–µ —Ü–µ–ª–æ–µ —á–∏—Å–ª–æ: "))
if num < 0:
  result = abs(num)
elif num == 0:
  result = 1
else:
  result = num
print(result)

#–ó–∞–¥–∞–Ω–∏–µ 2

string = input("–í–≤–µ–¥–∏—Ç–µ —Å—Ç—Ä–æ–∫—É: ")
if "." in string or "," in string:
    print(True)
else:
    print(False)
#–ó–∞–¥–∞–Ω–∏–µ 3
num1 = int(input("–í–≤–µ–¥–∏—Ç–µ –ø–µ—Ä–≤–æ–µ —á–∏—Å–ª–æ: "))
num2 = int(input("–í–≤–µ–¥–∏—Ç–µ –≤—Ç–æ—Ä–æ–µ —á–∏—Å–ª–æ: "))
div1_by_3 = (num1 % 3 == 0)
div2_by_3 = (num2 % 3 == 0)
if div1_by_3 and div2_by_3:
    print(True)
elif div1_by_3 or div2_by_3:
    print("–û–¥–Ω–æ —á–∏—Å–ª–æ –¥–µ–ª–∏—Ç—Å—è –Ω–∞ 3")
else:
    print(False)

#–í–∞—Ä–∏–∞–Ω—Ç 2
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ1
number = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: "))  
if number > 100:  
    print("*")  
elif number > 0:  
    print("*" * number)

#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ2
str1 = input("–í–≤–µ–¥–∏—Ç–µ –ø–µ—Ä–≤—É—é —Å—Ç—Ä–æ–∫—É: ")  
str2 = input("–í–≤–µ–¥–∏—Ç–µ –≤—Ç–æ—Ä—É—é —Å—Ç—Ä–æ–∫—É: ")  
if str1 == str2:
    print(True)
else:
    print(False)

#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ3
r = int(input("–í–≤–µ–¥–∏—Ç–µ –∑–Ω–∞—á–µ–Ω–∏–µ r (0-255): "))  
g = int(input("–í–≤–µ–¥–∏—Ç–µ –∑–Ω–∞—á–µ–Ω–∏–µ g (0-255): "))  
b = int(input("–í–≤–µ–¥–∏—Ç–µ –∑–Ω–∞—á–µ–Ω–∏–µ b (0-255): "))
if r == g == b == 0:
    print("–ß—ë—Ä–Ω—ã–π —Ü–≤–µ—Ç")
elif r == g == b == 255:
    print("–ë–µ–ª—ã–π —Ü–≤–µ—Ç")
elif r == 255 and g == b == 0:
    print("–ö—Ä–∞—Å–Ω—ã–π —Ü–≤–µ—Ç")
elif g == 255 and r == b == 0:
    print("–ó–µ–ª—ë–Ω—ã–π —Ü–≤–µ—Ç")
elif b == 255 and g == r == 0:
    print("–°–∏–Ω–∏–π —Ü–≤–µ—Ç")
else:
    print("–ù–µ—Ç —Ü–≤–µ—Ç–∞")

#–í–∞—Ä–∏–∞–Ω—Ç ‚Ññ3
#–ó–∞–¥–∞–Ω–∏–µ ‚Ññ1
num = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: "))
if num > 0:
    print(f"–†–µ–∑—É–ª—å—Ç–∞—Ç: {num - 1}, {num}, {num + 1}")
else:
    num = 1
    print(f"–†–µ–∑—É–ª—å—Ç–∞—Ç: {num - 1}, {num}, {num + 1}")
